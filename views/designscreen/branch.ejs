<strong id="tittle">Thông tin chi nhánh</strong>
<form action="#" method="POST" autocomplete="on">
    <input type="hidden" name="key" />
    <input type="text" id="name" class="form-control ctr-1x" name="namebranch" placeholder="Nhập tên chi nhánh ..." />
    <input type="text" id="address" class="form-control ctr-1x" name="addressbranch" placeholder="Địa chỉ ..." />
    <input type="tel" id="phone" class="form-control ctr-3x" name="phonebranch" placeholder="Số điện thoại ..." />
    <input type="email" id="email" class="form-control ctr-3x" name="emailbranch" placeholder="Email ..." />
    <input type="text" id="fax" class="form-control ctr-3x" name="faxbranch" placeholder="Số fax ..." />
    <button type="button" class="btn btn-danger btn-sm ctr-btn"><i class="fa fa-trash-o" aria-hidden="true"></i> Xóa Chi nhánh</button>
    <button type="button" class="btn btn-success btn-sm ctr-btn"><i class="fa fa-check" aria-hidden="true"></i> Cập nhật</button>
    
</form>
<div id="d-clear"></div>
<div class="table-responsive d-table">
    <table class="table table-hover table-condensed table-striped">
        <tr>
            <th>#</td>
            <th>Tên chi nhánh</th>
            <th>Địa chỉ</th>
            <th>Điện thoại</th>
            <th></th>
        </tr>
        <% for(i=1; i<100; i++) { %>
        <tr>
            <td><%= i %></td>
            <td>Tên chi nhánh</td>
            <td>Địa chỉ</td>
            <td class="col-phone">Điện thoại</td>
            <td class="col-action">
                <i class="fa fa-phone-square ctr-action" aria-hidden="true"></i>
                <i class="fa fa-envelope ctr-action" aria-hidden="true"></i>
                <i class="fa fa-trash ctr-action" aria-hidden="true"></i>
            </td>
        </tr>
       <% } %>
    </table>
</div>
