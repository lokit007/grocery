<!DOCTYPE html>
<html lang="en">
    <head>
        <title>Quản lý hệ thống tạp hóa</title>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link href="../css/bootstrap.min.css" rel="stylesheet" />
        <link href="../css/adminstyle.css" rel="stylesheet" />
        <script src="../js/jquery-3.1.1.min.js"></script>
        <script src="../js/jquery.validate.min.js"></script>
        <script src="../js/bootstrap.min.js"></script>
        <script src="../js/admin.js"></script>
        <link href="../font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    </head>
    <body>
        <div id="d-contain">
            <div id="d-navigation">
                <div id="menu"> Menu <i id="btn-show-menu" class="fa fa-bars" aria-hidden="true"></i></div>
                <ul id="sub-memu">
                    <li data="0" <% if(screen === 0) { %> class="active" <% } %>><i class="fa fa-university" aria-hidden="true"></i> Chi nhánh</li>
                    <li data="1" <% if(screen === 1) { %> class="active" <% } %>><i class="fa fa-address-card" aria-hidden="true"></i> Nhân viên</li>
                    <li data="2" <% if(screen === 2) { %> class="active" <% } %>><i class="fa fa-object-group" aria-hidden="true"></i> Danh mục</li>
                    <li data="3" <% if(screen === 3) { %> class="active" <% } %>><i class="fa fa-handshake-o" aria-hidden="true"></i> Đối tác</li>
                    <li data="4" <% if(screen === 4) { %> class="active" <% } %>><i class="fa fa-database" aria-hidden="true"></i> Kho hàng</li>
                    <li data="5" <% if(screen === 5) { %> class="active" <% } %>><i class="fa fa-area-chart" aria-hidden="true"></i> Thống kê</li>
                    <li data="6" <% if(screen === 6) { %> class="active" <% } %>><i class="fa fa-envelope" aria-hidden="true"></i> Tin nhắn</li>
                </ul>
            </div>
            <div id="d-content">
                <% if(screen === 0) { %>
                    <% include designscreen/branch.ejs %>
                <% } else if(screen === 1) { %>
                    <% include designscreen/personnel.ejs %>
                <% } else if(screen === 2) { %>
                    <% include designscreen/category.ejs %>
                <% } else if(screen === 3) { %>
                    <% include designscreen/branch.ejs %>
                <% } else if(screen === 4) { %>
                    <% include designscreen/branch.ejs %>
                <% } else if(screen === 5) { %>
                    <% include designscreen/branch.ejs %>
                <% } else if(screen === 6) { %>
                    <% include designscreen/branch.ejs %>
                <% } else { %>
                    Không có dữ liệu
                <% } %>
            </div>
            <div class="d-clear"></div>
        </div>

        // Model
        <div id="mdShowMes" class="modal fade bs-example-modal-sm" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-sm">
            <div id="md-div" class="modal-content modal-error">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                    <h4 class="modal-title">
                        <i id="md-title" class="fa fa-exclamation-triangle" aria-hidden="true"> Tiêu đề Mes</i>
                    </h4>
                </div>
                <div class="modal-body">
                    <p id="md-content">Nội dung tin nhắn</p>
                </div>
            </div>
        </div>
        </div>
    </body>
</html>